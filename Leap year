year=2001
if(year %4)==0:
    if(year%100)==0:
        if(year%400)==0:
            print("{0} is a leap year".format(year))
        else:
            print("{0} is not a leap year".formmat(year))
    else:
        print("{0} is a leap year".format(year))
else:
       print("{0} is not a leap year".format(year))
       
       
       
